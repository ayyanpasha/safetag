{
  "name": "@safetag/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "db:generate": "prisma generate --schema src/prisma/schema.prisma",
    "db:push": "prisma db push --schema src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/prisma/schema.prisma"
  },
  "dependencies": {
    "@safetag/service-utils": "workspace:*",
    "@safetag/shared-types": "workspace:*",
    "fastify": "^5.2.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^13.0.0",
    "@fastify/rate-limit": "^10.2.1",
    "@fastify/websocket": "^11.0.1",
    "@fastify/multipart": "^9.0.1",
    "fastify-plugin": "^5.0.1",
    "@prisma/client": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "razorpay": "^2.9.4",
    "ioredis": "^5.4.1",
    "zod": "^3.23.8",
    "uuid": "^10.0.0",
    "nanoid": "^5.0.9",
    "helmet": "^8.0.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "prisma": "^6.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.0.0",
    "pino-pretty": "^11.3.0",
    "@types/node": "^22.10.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13"
  }
}
